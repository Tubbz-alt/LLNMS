<!-- systemd control script.-->
<llnms-task>

    <!-- ID -->
    <id>logged-in</id>

    <!-- Description -->
    <description>Get a list of currently logged in users.</description>

    <!-- Configuration -->
    <configuration>

        <!-- Linux Configuration -->
        <linux>

            <!-- Flag if we are supporting the task for the requested OS-->
            <supported value="true" />

            <!-- Command -->
            <command name="who" />
            
        </linux>

    </configuration>
</llnms-task>
